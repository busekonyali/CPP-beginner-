#include <iostream>

using namespace std;

int main()
{
  int i,j,sum=0,number;
  cout<<"enter number"<<endl;
  cin>>number;
  for(i=1;i<=number;i++){
    sum=0;
  for(j=1;j<=i/2;j++){
    if(i%j==0){
        sum+=j;
    }
  }
    if(sum==i){
    cout<<i<<" is perfect number"<<endl;

  }


  }
    return 0;
}
