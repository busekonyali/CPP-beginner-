#include <iostream>

using namespace std;

int main()
{
    int arr[5]={1,2,3,4,5};
    int* ptr=arr;
    int** ptr2=&ptr;

    cout<<"array"<<endl;
      for (int i = 0; i < 5; i++) {
        cout << arr[i] << " ";
    }
    cout << endl;
cout<<"ters"<<endl;
  for (int i = 4; i >=0; i--) {
        cout << *(*ptr2+i) << " ";
    }


    return 0;
}
