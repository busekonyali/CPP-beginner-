\\ fiboyu macro olarak tanÄ±mlama
#include <iostream>

using namespace std;
#define FIB(first, second, third) { \
    third = first + second;         \
    first = second;                 \
    second = third;                 \
}

int main() {
    int first = 1, second = 1, third, num;

    cout << "Enter number: " << endl;
    cin >> num;

    cout << "1 1 ";

    if (num > 2) {
        for (int i = 3; i <= num; i++) {
            FIB(first, second, third);
            cout << third << " ";
        }
    }

    return 0;
}
