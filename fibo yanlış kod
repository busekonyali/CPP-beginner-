#include <iostream>

using namespace std;
int fibo(int x);
int main()
{
    int x;
    cout<<"enter terim num "<<endl;
    cin>>x;

    cout<<fibo(x)<<" ";

    return 0;
}
int fibo(int x){
    int i;
    int first=1,second=1,third=2;
    if(x==1)
        return first;
        if(x==2)
        return second;
  for(i=3;i<=x;i++){
    first=second;
    second=third;
    third=second+first;
  }
return third;



}
