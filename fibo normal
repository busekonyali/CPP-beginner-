#include <iostream>

using namespace std;

int main()
{
    int first=1,second=1,third,num,i;
    cout<<"enter number"<<endl;
    cin>>num;
    cout<<"1 1 ";
    if(num>2){
    first=1;
    second=1;
    for(i=3;i<num;i++){
        third=second+first;
        cout<<third<<" ";
        first=second;
        second=third;

    }
    }

    return 0;
}
